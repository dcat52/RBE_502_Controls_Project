
<!-- Starts up everything needed to run the example listener/talker Python nodes. -->
<launch>

	<node pkg="vicon_bridge" type="vicon_bridge" name="vicon" output="screen">
	<param name="stream_mode" value="ClientPull" type="str" />
	<param name="datastream_hostport" value="192.168.0.1:801" type="str" /> <!-- set vicon ip address on this line -->
	<param name="tf_ref_frame_id" value="/world" type="str" />
	</node>

	<!-- The listener node subscribes to the custom message that talker is publishing. -->
	<node pkg="controls" type="Controller.py" name="Controller"/>
</launch>