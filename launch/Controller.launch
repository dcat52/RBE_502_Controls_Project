
<!-- Starts up everything needed to run the example listener/talker Python nodes. -->
<launch>

	<arg name="ctrl" default="PID"/>
	<arg name="sim" default="True"/>
	<param name="cntrllr" value="$(arg ctrl)"/>
	<param name="simulation" value="$(arg sim)"/>

	<!-- The listener node subscribes to the custom message that talker is publishing. -->
	<node pkg="controls" type="Controller.py" name="Controller" output="screen" />
</launch>